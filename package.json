{
  "name": "create-permaweb-app",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "create-permaweb-app",
    "web3",
    "boilerplate",
    "nextjs",
    "vite",
    "react"
  ],
  "scripts": {
    "dev": "yarn workspace @create-permaweb-app/next dev",
    "build": "yarn workspace @create-permaweb-app/next build",
    "start": "yarn workspace @create-permaweb-app/next start",
    "lint": "yarn workspace @create-permaweb-app/next lint",
    "dev:ts": "yarn workspace @create-permaweb-app/next-ts dev",
    "build:ts": "yarn workspace @create-permaweb-app/next-ts build",
    "start:ts": "yarn workspace @create-permaweb-app/next-ts start",
    "lint:ts": "yarn workspace @create-permaweb-app/next-ts lint",
    "dev:vite": "yarn workspace @create-permaweb-app/vite dev",
    "build:vite": "yarn workspace @create-permaweb-app/vite build",
    "preview:vite": "yarn workspace @create-permaweb-app/vite preview",
    "dev:vite:ts": "yarn workspace @create-permaweb-app/vite-ts dev",
    "build:vite:ts": "yarn workspace @create-permaweb-app/vite-ts build",
    "preview:vite:ts": "yarn workspace @create-permaweb-app/vite-ts preview",
    "update-version": "lerna version --force-publish --no-private",
    "publish-latest": "lerna publish from-package",
    "create-permaweb-app": "node ./packages/create-permaweb-app/index.js",
    "dev:docs": "yarn workspace @create-permaweb-app-docs/docs dev"
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  },
  "workspaces": [
    "packages/*",
    "packages/create-permaweb-app/*",
    "packages/create-permaweb-app/templates/next/*",
    "packages/create-permaweb-app/templates/vite/*",
    "docs/"
  ]
}